{
    "presets": [
        [
            "@babel/preset-env",
            {
                "targets": {
                    "edge": 13,
                    "chrome": 43,
                    "ie": 11
                },
                "modules": false,
                "useBuiltIns": "usage",
                "debug": false,
                "forceAllTransforms": true,
                "corejs": {
                    "version": 3,
                    "proposals": true
                }
            }
        ],
        "@babel/preset-react"
    ],
    "plugins": [
        [
            "@babel/plugin-proposal-object-rest-spread",
            {
                "useBuiltIns": true
            }
        ],
        "@babel/plugin-proposal-optional-chaining",
        "@babel/plugin-transform-modules-commonjs",
        "babel-plugin-styled-components"
    ]
}